+COMMENT_RE = re.compile('<!-- +(\(UTC.*) -->.*')
+MAP_RE = re.compile('<mapZone other="(.*)" territory=".*" type="(.*)"/>')
+
+# Based on https://pubs.opengroup.org/onlinepubs/9699919799/ (variable TZ)
+def read_std(footer):
+    if footer[0] == '<':
+        for i in range(len(footer)):
+            if footer[i] == '>':
+                return footer[1:i], footer[i+1:]
+        raise Exception("Cannot find >")
+    else:
+        for i in range(len(footer)):
+            if not('A' <= footer[i] <= 'Z' or 'a' <= footer[i] <= 'z'):
+                break
+        return footer[:i], footer[i:]
+
+def read_number(footer):
+    for i in range(len(footer)):
+        if not('0' <= footer[i] <= '9'):
+            break
+    else:
+        i = len(footer)
+    return int(footer[:i]), footer[i:]
+
+def read_time(footer):
+    minutes = 0
+    seconds = 0
+    hours, footer = read_number(footer)
+    if len(footer) > 0 and footer[0] == ':':
+        footer = footer[1:]
+        minutes, footer = read_number(footer)
+        if len(footer) > 0 and footer[0] == ':':
+            footer = footer[1:]
+            seconds, footer = read_number(footer)
+    return (seconds + minutes * 60 + hours * 3600), footer
+
+def read_offset(footer):
+    sign = '+'
+    if footer[0] in ['+', '-']:
+        sign = footer[0]
+        footer = footer[1:]
+    time, footer = read_time(footer)
+    return ((-1 if sign == '-' else 1) * time), footer
+
+def read_date(footer):
+    if footer[0] == 'J':
+        footer = footer[1:]
+        num, footer = read_number(footer)
+        return ('J', num), footer
+    elif footer[0] == 'M':
+        footer = footer[1:]
+        month, footer = read_number(footer)
+        assert footer[0] == '.'
+        footer = footer[1:]
+        week, footer = read_number(footer)
+        assert footer[0] == '.'
+        footer = footer[1:]
+        day, footer = read_number(footer)
+        return ('M', month, week, day), footer
+    else:
+        num, footer = read_number(footer)
+        return ('D', num), footer
+
+def fix_date_for_tzi(time, date):
+    date = list(date)
+    days = time // (24 * 60 * 60)
+    date[3] = (date[3] + days) % 7
+    if days >= 4:
+        # Pass to the following week
+        date[2] += 1
+        if date[2] >= 5:
+            # Pass to the following month
+            if date[1] < 12:
+                date[2] = 1
+                date[1] += 1
+            else:
+                # Cannot pass to the following year, just take last
+                # week in month
+                date[2] = 5
+    if days <= -4:
+        # Pass to the previous week
+        date[2] -= 1
+        if date[2] <= 0:
+            # Pass to the previous month
+            if date[1] > 1:
+                date[2] = 5
+                date[1] -= 1
+            else:
+                # Cannot pass to the previous year, just take first
+                # week in month
+                date[2] = 1
+    return time % (24 * 60 * 60), tuple(date)
+
+def decode_footer(footer):
+    #print(footer)
+    std, footer = read_std(footer)
+    #print(std)
+    std_offset, footer = read_offset(footer)
+    #print(std_offset)
+    if footer != '':
+        dst, footer = read_std(footer)
+        #print(dst)
+        if footer[0] != ',':
+            dst_offset, footer = read_offset(footer)
+        else:
+            dst_offset = std_offset - 3600
+        #print(dst_offset)
+        assert footer[0] == ','
+        footer = footer[1:]
+        dst_date, footer = read_date(footer)
+        #print(dst_date)
+        if footer[0] == '/':
+            footer = footer[1:]
+            dst_time, footer = read_offset(footer)
+        else:
+            dst_time = 2 * 3600
+        #print(dst_time)
+        assert footer[0] == ','
+        footer = footer[1:]
+        std_date, footer = read_date(footer)
+        #print(std_date)
+        if len(footer) > 0 and footer[0] == '/':
+            footer = footer[1:]
+            std_time, footer = read_offset(footer)
+        else:
+            std_time = 2 * 3600
+        #print(std_time)
+        assert footer == ''
+    else:
+        dst = None
+        dst_offset = None
+        dst_date = None
+        dst_time = None
+        std_date = None
+        std_time = None
+
+    return std, std_offset, dst, dst_offset, dst_date, dst_time, std_date, std_time
+
+# Create the TZI structure according to https://docs.microsoft.com/en-us/windows/win32/api/timezoneapi/ns-timezoneapi-time_zone_information
+def footer_to_tzi(footer):
+    std, std_offset, dst, dst_offset, dst_date, dst_time, std_date, std_time = footer
+    if dst is not None:
+        assert std_offset % 60 == 0
+        assert dst_offset % 60 == 0
+        tzi = struct.pack("<lll", std_offset // 60, 0, (dst_offset - std_offset) // 60)
+        if std_date[0] == 'M':
+            std_time, std_date = fix_date_for_tzi(std_time, std_date)
+            tzi += struct.pack("<hhhhhhhh", 0, std_date[1], std_date[3], std_date[2], std_time // 3600, (std_time // 60) % 60, std_time % 60, 0)
+        else:
+            print('# Windows format cannot represent fixed dates')
+            tzi += struct.pack("<hhhhhhhh", 0, 0, 0, 0, 0, 0, 0, 0)
+        if dst_date[0] == 'M':
+            dst_time, dst_date = fix_date_for_tzi(dst_time, dst_date)
+            tzi += struct.pack("<hhhhhhhh", 0, dst_date[1], dst_date[3], dst_date[2], dst_time // 3600, (dst_time // 60) % 60, dst_time % 60, 0)
+        else:
+            print('# Windows format cannot represent fixed dates')
+            tzi += struct.pack("<hhhhhhhh", 0, 0, 0, 0, 0, 0, 0, 0)
+    else:
+        assert std_offset % 60 == 0
+        tzi = struct.pack("<lll", std_offset // 60, 0, -60)
+        tzi += struct.pack("<hhhhhhhh", 0, 0, 0, 0, 0, 0, 0, 0)
+        tzi += struct.pack("<hhhhhhhh", 0, 0, 0, 0, 0, 0, 0, 0)
+    #print(",".join(["{:02x}".format(x) for x in tzi]))
+    return tzi
+
+def realize_date(year, month, weekday, day):
+    diff = (weekday - datetime.date(year=year, month=month, day=1).weekday()) % 7
+    actual_day = 1 + diff + 7 * (day-1)
+    try:
+        date = datetime.datetime(year=year, month=month, day=actual_day, hour=0, minute=0, second=0, microsecond=0)
+    except ValueError:
+        date = datetime.datetime(year=year, month=month, day=actual_day-7, hour=0, minute=0, second=0, microsecond=0)
+    return date
+
+def realize_footer_date(year, footer_date):
+    if footer_date[0] == 'M':
+        return realize_date(year, footer_date[1], (footer_date[3]-1) % 7, footer_date[2])
+
+    elif footer_date[0] == 'J':
+        # Use 2021 as the model non-leap year
+        tmp = datetime.date(year=2021, month=1, day=1) + datetime.timedelta(days=footer_date[1]-1)
+        return datetime.date(year=year, month=tmp.month, day=tmp.day)
+
+    elif footer_date[0] == 'D':
+        return datetime.date(year=year, month=1, day=1) + datetime.timedelta(days=footer_date[1])
+
+    else:
+        assert False
