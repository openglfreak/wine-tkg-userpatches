          { SECTION, KEY,  NULL,          TESTFILE, -1, 4294967295U},
          { SECTION, KEY,  NULL,          TESTFILE,  1, 1},
+         { NULL,    NULL, "1",           NULL,     70, 0},           /* 15 */
+         { SECTION, NULL, "-1",          NULL,     -1, 0},
+         { SECTION, NULL, "-1",          NULL,      1, 0},
+         { NULL,    KEY,  "-1",          NULL,     -1, 0},
