          { SECTION, KEY,  "4294967296",  TESTFILE,  1, 0},
          { SECTION, KEY,  "4294967297",  TESTFILE, -1, 1},
-         { SECTION, KEY,  "4294967297",  TESTFILE,  1, 1},
