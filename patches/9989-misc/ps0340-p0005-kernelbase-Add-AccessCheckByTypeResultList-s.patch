+    FIXME( "stub (%p,%p,%p,%08x,%p,%08x,%p,%p,%p,%p,%p)\n", descr,
+           sid, token, access, types, types_len, mapping, privs,
+           priv_len, access_list, status_list );
+    return TRUE;
+}
