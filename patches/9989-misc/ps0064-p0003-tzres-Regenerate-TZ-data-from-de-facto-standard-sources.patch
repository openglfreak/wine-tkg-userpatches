+#         #tzi += struct.pack("<hhhhhhhh", 0, std_date[1], std_date[3], std_date[2], std_time // 3600, (std_time // 60) % 60, std_time % 60, 0)
+#         #tzi += struct.pack("<hhhhhhhh", 0, dst_date[1], dst_date[3], dst_date[2], dst_time // 3600, (dst_time // 60) % 60, dst_time % 60, 0)
+#     else:
+#         assert std_offset % 60 == 0
+#         tzi = struct.pack("<lll", std_offset // 60, 0, -60)
+#         tzi += struct.pack("<hhhhhhhh", 0, 0, 0, 0, 0, 0, 0, 0)
+#         tzi += struct.pack("<hhhhhhhh", 0, 0, 0, 0, 0, 0, 0, 0)
+#     return tzi
+
+def realize_tzi(tzi, year):
+    data = list(struct.unpack("<lllhhhhhhhhhhhhhhhh", tzi))
+    for i in 0, 8:
+        if data[3+i] == 0 and data[4+i] != 0:
+            month = data[4+i]
+            weekday = (data[5+i]-1) % 7
+            day = data[6+i]
+            hour = data[7+i]
+            minute = data[8+i]
+            second = data[9+i]
+            millisecond = data[10+i]
+            date = realize_date(year, month, weekday, day)
+            date += datetime.timedelta(hours=hour, minutes=minute, seconds=second, microseconds=millisecond * 1000)
+            data[3+i] = date.year
+            data[4+i] = date.month
+            data[5+i] = (date.weekday()+1) % 7
+            data[6+i] = date.day
+            data[7+i] = date.hour
+            data[8+i] = date.minute
+            data[9+i] = date.second
+            data[10+i] = date.microsecond // 1000
+    return struct.pack("<lllhhhhhhhhhhhhhhhh", *data)
+
+# It mimicks std::lower_bound from C++, though it checks v[x][0], not
+# v[x]
+def lower_bound(v, x):
+    low, high = 0, len(v)
+    while low != high:
+        i = (low + high) // 2
+        if v[i][0] < x:
+            low = i + 1
+        else:
+            high = i
+    if low != len(v):
+        assert not v[low][0] < x
+    if low != 0:
+        assert v[low-1][0] < x
+    return low
+
+def date_to_datetime(date):
+    return datetime.datetime(year=date.year, month=date.month, day=date.day, hour=0, minute=0, second=0, microsecond=0)
+
+FIRST_YEAR = 2000
+LAST_YEAR = 2050
+
+# Based on https://tools.ietf.org/html/rfc8536
+def read_tz(filename):
+    with open(filename, 'rb') as fin:
+        header1 = fin.read(44)
+        assert header1[:4] == b'TZif'
+        assert header1[4:5] == b'2' or header1[4:5] == b'3'
+        isutcnt, isstdcnt, leapcnt, timecnt, typecnt, charcnt = struct.unpack("!IIIIII", header1[20:])
+        fin.read(timecnt * 4 + timecnt + typecnt * 6 + charcnt + leapcnt * 8 + isstdcnt + isutcnt)
+        header2 = fin.read(44)
+        assert header2[:4] == b'TZif'
+        assert header2[4:5] == header1[4:5]
+        isutcnt, isstdcnt, leapcnt, timecnt, typecnt, charcnt = struct.unpack("!IIIIII", header2[20:])
+
+        transition_times = [struct.unpack("!q", fin.read(8))[0] for _ in range(timecnt)]
+        transition_types = [struct.unpack("B", fin.read(1))[0] for _ in range(timecnt)]
+        local_times = [struct.unpack("!i?B", fin.read(6)) for _ in range(typecnt)]
+        designations = fin.read(charcnt).decode('ascii')
+        leap_seconds = [struct.unpack("!Qi", fin.read(12)) for _ in range(leapcnt)]
+        std_indicators = [struct.unpack("?", fin.read(1))[0] for _ in range(isstdcnt)]
+        ut_indicators = [struct.unpack("?", fin.read(1))[0] for _ in range(isutcnt)]
+
+        if std_indicators == []:
+            std_indicators = [False] * typecnt
+        if ut_indicators == []:
+            ut_indicators = [False] * typecnt
+        assert len(std_indicators) == len(local_times)
+        assert len(ut_indicators) == len(local_times)
+        assert len(leap_seconds) == 0
+
+        print("# Local times:")
+        descriptions = []
+        utoffs = []
+        dsts = []
+        desigs = []
+        for i in range(len(local_times)):
+            utoff, dst, idx = local_times[i]
+            for idx_end in range(idx, len(designations)):
+                if designations[idx_end] == '\0':
+                    break
+            designation = designations[idx:idx_end]
+            isstd = std_indicators[i]
+            isut = ut_indicators[i]
+            descr = "{}: {} (UT{:+}, {}) isstd={} isut={}".format(i+1, designation, utoff / 3600, 'daylight' if dst else 'standard', isstd, isut)
+            descriptions.append(descr)
+            utoffs.append(utoff)
+            dsts.append(dst)
+            desigs.append(designation)
+            print("# {}".format(descr))
+        print("# Transitions:")
+        print("# Beginning {}".format(descriptions[0]))
+        prev_tz = datetime.timezone(datetime.timedelta(seconds=utoffs[0]), name=desigs[0])
+        prev_naive = None
+        local_changes = []
+        for i in range(len(transition_times)):
+            change_utc = datetime.datetime.fromtimestamp(transition_times[i], tz=datetime.timezone.utc)
+            change_prev = datetime.datetime.fromtimestamp(transition_times[i], tz=prev_tz)
+            change_prev_naive = change_prev.replace(tzinfo=None)
+            if prev_naive is not None:
+                assert change_prev_naive > prev_naive
+            local_changes.append((change_prev_naive, utoffs[transition_types[i]]))
+            prev_naive = change_prev_naive
+            print("# {} ({}, corresponding to {}) to {}".format(transition_times[i], change_utc, change_prev, descriptions[transition_types[i]]))
+            prev_tz = datetime.timezone(datetime.timedelta(seconds=utoffs[transition_types[i]]), name=desigs[transition_types[i]])
+
+        assert fin.read(1) == b'\n'
+        footer = fin.readline().decode('ascii').strip()
+        print("# Footer: {}".format(footer))
+        footer_data = decode_footer(footer)
+        std, std_offset, dst, dst_offset, dst_date, dst_time, std_date, std_time = footer_data
+        tzi = footer_to_tzi(footer_data)
+
+        # Complement local_changes with transitions generated with the
+        # footer
+        if dst is None:
+            if local_changes == []:
+                local_changes.append((datetime.datetime(year=FIRST_YEAR, month=1, day=1, hour=0, minute=0, second=0, microsecond=0), -std_offset))
+        else:
+            first_year = FIRST_YEAR
+            if local_changes != [] and local_changes[-1][0].year < first_year:
+                first_year = local_changes[-1][0].year
+            for year in range(first_year, LAST_YEAR):
+                start_of_year = datetime.datetime(year=year, month=1, day=1, hour=0, minute=0, second=0, microsecond=0)
+                actual_std_date = realize_footer_date(year, std_date)
+                actual_dst_date = realize_footer_date(year, dst_date)
+                change1 = (date_to_datetime(actual_std_date) + datetime.timedelta(seconds=std_time), -std_offset)
+                change2 = (date_to_datetime(actual_dst_date) + datetime.timedelta(seconds=dst_time), -dst_offset)
+                if change2[0] < change1[0]:
+                    change1, change2 = change2, change1
+                if local_changes == [] and change1[0] != start_of_year:
+                    local_changes.append((start_of_year, change2[1]))
+                if change1[0] > local_changes[-1][0]:
+                    local_changes.append(change1)
+                if change2[0] > local_changes[-1][0]:
+                    local_changes.append(change2)
+
+        # print("# Local changes:")
+        # for change in local_changes:
+        #     print("# {} to {}".format(change[0], change[1]))
+
+        # For each year in a reasonable range, check if there is need
+        # for a dynamic rule
+        dynamic_tzis = []
+        for year in range(FIRST_YEAR, LAST_YEAR):
+            start_of_year = datetime.datetime(year=year, month=1, day=1, hour=0, minute=0, second=0, microsecond=0)
+            start_idx = lower_bound(local_changes, start_of_year)
+            assert start_idx < len(local_changes) or dst is None
+            if start_idx == len(local_changes):
+                # Past the end of recorded data, so the footer is in
+                # force
+                pass
+            else:
+                if local_changes[start_idx][0] != start_of_year:
+                    assert start_idx > 0
+                    start_idx -= 1
+                for end_idx in range(start_idx+1, len(local_changes)):
+                    if local_changes[end_idx][0].year != year:
+                        break
+                else:
+                    end_idx = len(local_changes)
+                relevant_changes = []
+                for i in range(start_idx, end_idx):
+                    if i == start_idx or local_changes[i][1] != local_changes[i-1][1]:
+                        relevant_changes.append(local_changes[i])
+
+                if len(relevant_changes) > 3:
+                    print("# WARNING! Too many changes in {}, will drop some of them".format(year))
+                    relevant_changes = [relevant_changes[0], relevant_changes[1], relevant_changes[-1]]
+
+                if len(relevant_changes) == 1:
+                    [change] = relevant_changes
+                    assert change[1] % 60 == 0
+                    year_tzi = struct.pack("<lll", -change[1] // 60, 0, -60)
+                    year_tzi += struct.pack("<hhhhhhhh", 0, 0, 0, 0, 0, 0, 0, 0)
+                    year_tzi += struct.pack("<hhhhhhhh", 0, 0, 0, 0, 0, 0, 0, 0)
+
+                elif len(relevant_changes) == 2:
+                    [change1, change2] = relevant_changes
+                    if change1[1] == change2[1]:
+                        print("# WARNING! Not changing utoff in {}".format(year))
+                    if change1[1] > change2[1]:
+                        dst_change = change1
+                        std_change = change2
+                    else:
+                        std_change = change1
+                        dst_change = change2
+                    if std_change[0].year != year:
+                        std_change = (start_of_year, std_change[1])
+                    if dst_change[0].year != year:
+                        dst_change = (start_of_year, dst_change[1])
+                    assert std_change[1] % 60 == 0
+                    assert dst_change[1] % 60 == 0
+                    std_week = (std_change[0].day-1) // 7 + 1
+                    # if std_week == 4:
+                    #     std_week = 5
+                    dst_week = (dst_change[0].day-1) // 7 + 1
+                    # if dst_week == 4:
+                    #     dst_week = 5
+                    year_tzi = struct.pack("<lll", -std_change[1] // 60, 0, -(dst_change[1] - std_change[1]) // 60)
+                    year_tzi += struct.pack("<hhhhhhhh", 0, std_change[0].month, (std_change[0].weekday()+1) % 7, std_week,
+                                            std_change[0].hour, std_change[0].minute, std_change[0].second, std_change[0].microsecond // 1000)
+                    year_tzi += struct.pack("<hhhhhhhh", 0, dst_change[0].month, (dst_change[0].weekday()+1) % 7, dst_week,
+                                            dst_change[0].hour, dst_change[0].minute, dst_change[0].second, dst_change[0].microsecond // 1000)
+
+                elif len(relevant_changes) == 3:
+                    [change1, change2, change3] = relevant_changes
+                    if change1[1] == change2[1]:
+                        print("# WARNING! Not changing utoff in {}".format(year))
+                    if change2[1] == change3[1]:
+                        print("# WARNING! Not changing utoff in {}".format(year))
+                    if change1[1] != change3[1]:
+                        print("# WARNING! Utoff doesn't switch back and forth in {}".format(year))
+                    if change2[1] > change3[1]:
+                        dst_change = change2
+                        std_change = change3
+                    else:
+                        std_change = change2
+                        dst_change = change3
+                    assert std_change[1] % 60 == 0
+                    assert dst_change[1] % 60 == 0
+                    std_week = (std_change[0].day-1) // 7 + 1
+                    # if std_week == 4:
+                    #     std_week = 5
+                    dst_week = (dst_change[0].day-1) // 7 + 1
+                    # if dst_week == 4:
+                    #     dst_week = 5
+                    year_tzi = struct.pack("<lll", -std_change[1] // 60, 0, -(dst_change[1] - std_change[1]) // 60)
+                    year_tzi += struct.pack("<hhhhhhhh", 0, std_change[0].month, (std_change[0].weekday()+1) % 7, std_week,
+                                            std_change[0].hour, std_change[0].minute, std_change[0].second, std_change[0].microsecond // 1000)
+                    year_tzi += struct.pack("<hhhhhhhh", 0, dst_change[0].month, (dst_change[0].weekday()+1) % 7, dst_week,
+                                            dst_change[0].hour, dst_change[0].minute, dst_change[0].second, dst_change[0].microsecond // 1000)
+
+                else:
+                    assert(False)
+
+                if realize_tzi(tzi, year) != realize_tzi(year_tzi, year):
+                    dynamic_tzis.append((year, year_tzi))
+
+        # If there are dynamic TZI entries at all, add another one at
+        # the end with the plain tzi data, because Windows does the
+        # same
+        if dynamic_tzis != []:
+            dynamic_tzis.append((dynamic_tzis[-1][0]+1, tzi))
+
+        return tzi, dynamic_tzis
+
+def main():
+    command = sys.argv[1]
+    windows_zones_filename = sys.argv[2]
+    tzdata_dir = sys.argv[3]
+    backward_filename = sys.argv[4]
+
+    backward_data = {}
+    with open(backward_filename) as fin:
